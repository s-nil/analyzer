#ifndef _A_FORWARD_FLOW_ANALYSIS_H_
#define _A_FORWARD_FLOW_ANALYSIS_H_
#include "FlowAnalysis"

namespace A{
    /**
     * @brief 
     * 
     * @tparam T 
     */
    template<typename T>
    class ForwardFlowAnalysis : public FlowAnalysis<T>{
    public:
        ForwardFlowAnalysis(){}
        ~ForwardFlowAnalysis(){}

        void DoAnalysis() override;
        bool IsForward() override;
    private:
    };    

template<typename T>
void A::ForwardFlowAnalysis<T>::DoAnalysis(){
    this->Compute();
}

template<typename T>
inline bool A::ForwardFlowAnalysis<T>::IsForward(){
    return true;
}
}

#endif